{
  "name": "miguelpimentel.do_package",
  "version": "1.2.0",
  "description": "My personal website built using Eleventy with some Vite.",
  "browserslist": [
    "> 0.5%",
    "not IE 11"
  ],
  "scripts": {
    "start": "npx @11ty/eleventy --serve",
    "clean-build": "del-cli _site && npx @11ty/eleventy --watch --serve",
    "watch": "npx @11ty/eleventy --watch",
    "bench": "DEBUG=Eleventy:Benchmark* npx @11ty/eleventy",
    "build": "npx @11ty/eleventy",
    "clean": "del-cli _site",
    "test": "echo \"Error: no test specified\" && exit 1",
    "check": "npx prettier . --check",
    "format": "npx prettier . --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/semanticdata/miguelpimentel.do.git"
  },
  "author": "Miguel Pimentel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/semanticdata/miguelpimentel.do/issues"
  },
  "homepage": "https://miguelpimentel-do.vercel.app/",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^3.1.8",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@11ty/eleventy-plugin-vite": "^4.0.0",
    "autoprefixer": "^10.4.17",
    "cssnano": "^6.0.5",
    "cssremedy": "0.1.0-beta.2",
    "del-cli": "^5.1.0",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "focus-visible": "^5.2.0",
    "luxon": "^3.4.4",
    "markdown-it": "^13.0.2",
    "markdown-it-anchor": "^8.6.7",
    "npm-run-all": "^4.1.5",
    "postcss-nesting": "^12.0.3",
    "rollup-plugin-critical": "^1.0.13",
    "sass": "^1.71.1",
    "vite": "^4.5.2"
