    "serve": "npx @11ty/eleventy --serve",
    "build": "npx @11ty/eleventy",
    "clean-build": "del-cli _site && npx @11ty/eleventy",
    "clean": "del-cli _site",
    "test": "echo \"Error: no test specified\" && exit 1",
    "check": "npx prettier . --check",
    "format": "npx prettier . --write"
  },
  "repository": {
